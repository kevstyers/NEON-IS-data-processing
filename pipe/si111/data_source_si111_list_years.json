{
  "pipeline": {
    "name": "data_source_si111_list_years"
  },
  "transform": {
    "image":"registry.access.redhat.com/ubi8/ubi-minimal:8.3",
    "cmd": [
      "/bin/bash"
      ],
    "stdin": [
      "#!/bin/bash",
      "ls $REPO_IN > /pfs/out/data_years.txt"
      ]
  },
  "input": {
    "pfs": {
      "name": "REPO_IN",
      "repo": "data_source_si111_linkmerge",
      "glob": "/si111",
      "empty_files": true
    }
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "1K",
    "cpu": 0.01
  }
}
