{
  "pipeline": {
    "name": "pressureAir_surfacewaterPhysical_group_path"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/location_group_path:10",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m location_group_path.location_group_path_main"
    ],
    "env": {
      "GROUP": "barometric-pressure-",
      "LOG_LEVEL": "INFO",
      "OUT_PATH": "/pfs/out",
      "SOURCE_TYPE_INDEX": "3",
      "YEAR_INDEX": "4",
      "MONTH_INDEX": "5",
      "DAY_INDEX": "6",
      "LOCATION_INDEX": "7",
      "DATA_TYPE_INDEX": "8"
    }
  },
  "input": {
   "union": [
      {
        "pfs": {
          "name": "SOURCE_PATH",
          "repo": "surfacewaterPhysical_aquatroll200_level1_group",
          "glob": "/aquatroll200/*/*/*",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "SOURCE_PATH",
          "repo": "surfacewaterPhysical_leveltroll500_level1_group",
          "glob": "/leveltroll500/*/*/*",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true
}
